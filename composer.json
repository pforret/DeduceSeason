{
    "name": "pforret/deduceseason",
    "version": "1.0.1",
    "description": "deduce season from date and location",
    "keywords": [
        "pforret",
        "deduceseason"
    ],
    "homepage": "https://github.com/pforret/deduceseason",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^0.18.6",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Pforret\\DeduceSeason\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pforret\\DeduceSeason\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "setup-ci": "vendor/bin/rector setup-ci",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
